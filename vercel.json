{
  "functions": {
    "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/:type(.*).svg",
      "destination": "/api/:type.js"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/PonomareVlad/github-readme-stats"
    }
  ]
}
